<p id="notice"><%= notice %></p>

<body class='leaderboard'>
  <h1>LEADERBOARD</h1>
  <div class=''>
    <table class="table table-hover table-inverse">
      <thead class="thead-default">
        <tr>
          <th>Username</th>
          <th>Score</th>
          <th colspan="2"></th>
        </tr>
      </thead>

      <tbody>
        <% @highest_score.each do |user| %>
          <tr>
            <td><%= user.username %></td>
            <td><%= user.score %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
  <hr>
</body>
<br>
